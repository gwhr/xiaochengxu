/* 下拉 */
.seekPull{
  padding:34rpx;
  transition: all .3s ease-in-out 0s;
}
.seekPull .seekPull_content{
  text-align: center;
  line-height: 115rpx;
  font-size:26rpx;
  color:#8a42f5;
  position: relative;
  
}
.seekPull_content .seekPull_img{
  width:34rpx;
  height:34rpx;
  display: inline-block;
  position: absolute;
  top:10rpx;
}
.seekPull_img image{
  width:100%;
  height:100%;
}
.seekPull_content .seekPull_text{
  display: inline-block;
  margin-left:50rpx;
}

.newSeek{
  text-align: center;
  width:336rpx;
  height:100rpx;
  border:1rpx dashed #8a42f5;
  line-height: 100rpx;
  margin:0 auto;
  border-radius:80rpx;
  font-weight: bold;
  color:#8a42f5
}
/* 打野top */
.seekTop{
  width:100%;
  overflow: hidden;
  height:62rpx;
  padding:0 34rpx;
  box-sizing: border-box
}
.seekTop .seekTop_left{
  font-size:30rpx;
  font-weight: bold;
  float:left;
  line-height:62rpx;
}
.seekTop .seekTop_right{
  font-size:23rpx;
  float:right;
  line-height:62rpx;
  color:#5d5e5f;
}
